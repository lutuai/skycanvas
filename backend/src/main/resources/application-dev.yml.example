# 开发环境配置示例
# 复制此文件为 application-dev.yml 并填写实际配置

spring:
  datasource:
    url: jdbc:mysql://localhost:3306/skycanvas?useUnicode=true&characterEncoding=utf8&useSSL=false&serverTimezone=Asia/Shanghai
    username: root
    password: your_password
  
  redis:
    host: localhost
    port: 6379
    password: 

# 微信配置
wechat:
  miniapp:
    app-id: your_appid
    app-secret: your_secret
  payment:
    mch-id: your_mch_id
    api-key: your_pay_key
    notify-url: https://yourdomain.com/api/payment/notify

# 阿里云OSS
aliyun:
  oss:
    endpoint: oss-cn-hangzhou.aliyuncs.com
    access-key-id: your_access_key
    access-key-secret: your_access_secret
    bucket-name: skycanvas
    cdn-domain: https://cdn.yourdomain.com

# Sora API
video:
  provider:
    sora-proxy:
      api-key: your_sora_api_key
      base-url: https://api.soraapi.cn

